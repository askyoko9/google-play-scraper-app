{
  "functions": {
    "api/index.py": {
      "runtime": "python@3.9",
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/",
      "destination": "/api/index.py"
    }
  ]
}