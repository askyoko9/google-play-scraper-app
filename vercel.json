{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/",
      "destination": "/api/index.py"
    }
  ]
}